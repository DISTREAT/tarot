{
  "build": {
    "beforeDevCommand": "bun run dev",
    "beforeBuildCommand": "bun run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "tarot",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "process": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.distreat.tarot",
      "icon": ["icons/icon.png"]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "tarot",
        "width": 1600,
        "height": 900
      }
    ]
  }
}
